{
  "name": "blog-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "clear-docker-data": "docker system prune -f",
    "start:dev": "yarn clear-docker-data && docker-compose --env-file ./dotenv/development.env up --build web db",
    "start:prod": "yarn clear-docker-data && docker-compose --env-file ./dotenv/production.env up --build web-prod db-prod",
    "start-node-server:development": "nodemon -r dotenv_config_path=./dotenv/development.env",
    "start-node-server:production": "nodemon -r dotenv_config_path=./dotenv/production.env",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "migration-generate": "NODE_ENV=development yarn run typeorm migration:generate src/migrations/migration -d src/config/ormconfig.ts",
    "migration-run": "NODE_ENV=development yarn run typeorm migration:run -d src/config/ormconfig.ts",
    "migration-generate:production": "NODE_ENV=production yarn run typeorm migration:generate src/migrations/migration -d src/config/ormconfig.ts",
    "migration-run:production": "NODE_ENV=production yarn run typeorm migration:run -d src/config/ormconfig.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "lint-fix": "yarn lint --fix",
    "check-types": "tsc --noEmit",
    "precommit": "npx lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/typeorm": "^8.1.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.12",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "^0.3.6",
    "typescript": "^4.4.3",
    "winston": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MusicCorner/blog-api.git"
  },
  "author": "MusicCorner",
